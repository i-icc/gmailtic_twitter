{
    "manifest_version": 3,
    "name": "gmailtic twitter",
    "version": "0.1.0",
    "author": "i-icc",
    "description": "Twitter を gmail 風に表示します。",
    
    "icons": {
        "16": "images/icon.png"
    },

    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "css": [
                "css/index.css",
                "css/side_bar.css",
                "css/gmail.css"
            ],
            "js": [
                "js/index.js"
            ]
        }
    ]
}